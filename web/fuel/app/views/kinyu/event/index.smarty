<script>
$(function(){
    $('#gcarender').on('load', function(){
        $('#gcarender').contents().find('#currentDate1').css('font-size','16px');
        $('#gcarender').contents().find('#currentDate1').css('padding-left','0em');
        $('#gcarender').contents().find('#currentDate1').css('padding','5px');
        $('#gcarender').contents().find('#dateEditableBox1').css('display','block');
    });
});
</script>





