<script>
  $(function(){
    var coordinate = new google.maps.LatLng(35.680676, 139.779354);/*座標の指定*/
    var coordinate1 = new google.maps.LatLng(35.6826631, 139.7787332);/*座標の指定*/

    /*マップの設定*/
    var mapOptions = {
      zoom: 16, /*Map表示時の拡大倍率を調整。*/
      center: coordinate, /*中心点をどこにするか。この場合は、目的地と同じ*/
      scrollwheel: false,
      mapTypeId: google.maps.MapTypeId.ROADMAP/*地図の表示タイプの指定。*/
    };

    var mapOptions1 = {
      zoom: 16, /*Map表示時の拡大倍率を調整。*/
      center: coordinate1, /*中心点をどこにするか。この場合は、目的地と同じ*/
      scrollwheel: false,
      mapTypeId: google.maps.MapTypeId.ROADMAP/*地図の表示タイプの指定。*/
    };

    /*マップをID area-google-mapに埋め込む記述*/
    var map = new google.maps.Map(document.getElementById('area-google-map'), mapOptions);
    var map1 = new google.maps.Map(document.getElementById('area-google-map1'), mapOptions1);

    /*マップのデザインの指定*/
    var styleOptions = [
      {
        "stylers":  [
      { gamma: 0.99 },
      { visibility: "on" },
      { lightness: 26 },
      { hue: "#CFFFF7" },
      { saturation: -80 }
    ]
      }
    ];

    /*マップのデザインを適用させる記述*/
    var styleType = new google.maps.StyledMapType(styleOptions);
    map.mapTypes.set('genius', styleType);
    map.setMapTypeId('genius');
    map1.mapTypes.set('genius', styleType);
    map1.setMapTypeId('genius');

    /*アイコンの表示設定*/
    var icon = new google.maps.MarkerImage('/images/content/conference/location_cafesarvador1.png',/*アイコンのパス*/
      new google.maps.Size(50,50),/*アイコンのサイズ*/
      new google.maps.Point(0, 0)/*座標からのアイコンの位置*/
