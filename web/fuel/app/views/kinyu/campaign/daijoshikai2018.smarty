<script>
  $(function() {
    var hh = $(".main-out-event-gallery-list li").width();
    $('.main-out-event-gallery-list li').height(hh);
  });

  $(function(){
    $("#document-open").click(function() {
      $(".conference-document-form").fadeIn(500);
    });
    $("#document-open-closebtn").click(function() {
      $(".conference-document-form").fadeOut(500);
    });
    $("#document-form-after-closebtn").click(function() {
      $("#conference-document-form-after").fadeOut(500);
    });
  });

  $(function(){
    var coordinate = new google.maps.LatLng(35.6826631, 139.7787332);/*座標の指定*/

    /*マップの設定*/
    var mapOptions = {
      zoom: 16, /*Map表示時の拡大倍率を調整。*/
      center: coordinate, /*中心点をどこにするか。この場合は、目的地と同じ*/
      scrollwheel: false,
      mapTypeId: google.maps.MapTypeId.ROADMAP/*地図の表示タイプの指定。*/
    };

    /*マップをID area-google-mapに埋め込む記述*/
    var map = new google.maps.Map(document.getElementById('area-google-map'), mapOptions);

    /*マップのデザインの指定*/
    var styleOptions = [
      {
        "stylers":  [
      { gamma: 0.99 },
      { visibility: "on" },
      { lightness: 26 },
      { hue: "#CFFFF7" },
      { saturation: -80 }
    ]
      }
    ];

    /*マップのデザインを適用させる記述*/
    var styleType = new google.maps.StyledMapType(styleOptions);
    map.mapTypes.set('genius', styleType);
    map.setMapTypeId('genius');

    /*アイコンの表示設定*/
    var icon = new google.maps.MarkerImage('/images/content/conference/location_jpxarrows1.png',/*アイコンのパス*/
      new google.maps.Size(50,50),/*アイコンのサイズ*/
      new google.maps.Point(0, 0)/*座標からのアイコンの位置*/
    );
    
    var markerOptions = {
      position: coordinate,/*表示場所と同じ位置に設置*/
      map: map,
      icon: icon,
    };

    /*アイコンを表示させる記述*/
    var marker = new google.maps.Marker(markerOptions);
  });


  // 資料ダウンロード
  $(function(){
    var title_frame_height = $('#title_frame').height();
    var page_title_height = $('.page_title').height();
    
    var padding_top = (title_frame_height / 2) - (page_title_height / 2);
    $('.page_title').css('padding-top', padding_top);
    
    $('#save').click(function(){
      var url = "/api/regist/document";
      var params = {};
      
      params.name = $('#name').val();
      params.company = $('#company').val();
      params.email = $('#email').val();
      
      if (params.name == "") {
        ts.error("お名前を入力してください");
        return false;
      }
      if (params.company == "") {
        ts.error("会社名を入力してください");
        return false;
      }
      if (params.email == "") {
        ts.error("メールアドレスを入力してください。");
        return false;
      }

      bootbox.confirm("入力内容を送信しますか？", function(result) {
        if (result) {
          ajax.post(url, params, function(data){
            
            $(".conference-document-form").fadeOut(500);
            
            setTimeout(function(){
              $("#conference-document-form-after").fadeIn(500);
            },1000);
          
          });
        }
        else {
          ts.info("キャンセルしました");
        }
      });
      
    });
  });
</script>